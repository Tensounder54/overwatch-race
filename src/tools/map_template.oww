rule("Global state 0 - checkpoint position creation")
{
	event
	{
		Ongoing - Global;
	}

	conditions
	{
		Global Variable(Z) == 0;
	}

	actions
	{
		Set Global Variable(A, Empty Array);
		Modify Global Variable(A, Append To Array, Vector(0, 0, 0));
		Modify Global Variable(A, Append To Array, Vector(0, 0, 0));
		Modify Global Variable(A, Append To Array, Vector(0, 0, 0));
		Set Global Variable(Z, 1);
	}
}

