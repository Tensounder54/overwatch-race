		Objective Index < Add(Global Variable(E), Global Variable(F));
		Is On Objective(Event Player) == False;
	}

	actions
	{
		Teleport(Event Player, Payload Position);
		Wait(0.250, Ignore Condition);
		Loop If Condition Is True;
	}
}

